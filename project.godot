; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="minor-mechanic-library"
run/main_scene="res://Core/Scenes/catalog.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")

[autoload]

AudioPlayer="*res://Core/Scenes/AL_audio_player.tscn"
LocalDb="*res://Core/Scripts/LocalDB.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="minor-mechanic-library-testing"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/tmml_manager/plugin.cfg")

[gui]

theme/custom="res://Shared/tmml_theme.tres"

[rendering]

environment/defaults/default_clear_color=Color(0.133333, 0.137255, 0.137255, 1)
anti_aliasing/quality/msaa_2d=3
